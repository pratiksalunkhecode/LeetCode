class Solution {
    public int maximumDifference(int[] nums) {
        int n = nums.length;
          int res = nums[1] - nums[0];

          for(int i = 0; i < n-1; i++)
          {
            for(int j = i+1; j < n; j++)
            {
                res = Math.max(res, nums[j] - nums[i]);
            }

           
          }
return res;

    }
}
